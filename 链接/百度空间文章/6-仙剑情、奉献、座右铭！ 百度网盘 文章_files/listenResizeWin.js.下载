define(["lib/jquery/1_9"],function(e){var t={},n={};return t.callbackList=[],t.checkResize=function(){var r=e(window).width(),o=e(window).height(),i={width:r,height:o,oldWidth:n.width,oldHeight:n.height};return!t.callbackList.length||n.width==r&&n.height==o||t.runCallbackList(i),n.width=r,n.height=o,i},t.runCallbackList=function(e){var n,r;for(n=0,r=t.callbackList.length;r>n;++n)try{t.callbackList[n](e)}catch(o){}},t.listenResizeWin=function(){t.checkResize(),t.iSI=setInterval(function(){t.checkResize()},1e3),e(window).on("resize",t.checkResize)},n.add=function(e){t.callbackList.push(e),e({width:n.width,height:n.height,oldWidth:n.width,oldHeight:n.height})},n.remove=function(e){var n,r;for(n=0,r=t.callbackList.length;r>n;++n)if(t.callbackList[n]===e)return t.callbackList.splice(n,1),!0;return!1},n.clear=function(){t.callbackList=[]},n.destroy=function(){n.clear(),clearInterval(t.iSI),e(window).off("resize",t.checkResize)},t.listenResizeWin(),n});