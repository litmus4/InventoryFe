define(function(){var e={strTrim:function(e){return e.replace(/^\s+/,"").replace(/\s+$/,"")},removeRepeat:function(e){var t={},n=[];for(i=0,l=e.length;l>i;++i)e[i]&&!t[e[i]]&&(n.push(e[i]),t[e[i]]=!0);return n}},t=function(e){var n={},r={},i=function(){n.str=t.toArray(e.toString()).join(", ")};return r.add=function(e){return n.str=t.add(n.str,e),r},r.del=function(e){return n.str=t.del(n.str,e),r},r.toArray=function(){return t.toArray(n.str)},r.toString=function(){return n.str},i(),r};return t.add=function(n,r){var i=t.toArray(n);return r=e.strTrim(r),r&&i.push(r),i=e.removeRepeat(i),i.join(", ")},t.del=function(n,r){var i,o,s=t.toArray(n),a=e.strTrim(r),l=[];for(i=0,o=s.length;o>i;++i)s[i]!==a&&l.push(s[i]);return l=e.removeRepeat(l),l.join(", ")},t.toArray=function(t){t=t.replace(/[、，；;\|,]/g,",");var n,r,i=t.split(","),o=[],s={};for(n=0,r=i.length;r>n;++n)i[n]=e.strTrim(i[n]),i[n]&&!s[i[n]]&&(o.push(i[n]),s[i[n]]=!0);return o},t});